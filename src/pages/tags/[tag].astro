---
import BaseLayout from '../../layouts/BaseLayout.astro';

export async function getStaticPaths() {
    const allPosts = await Astro.glob('../../data/blog-posts/*.md');
    return [
        {params: {tag: "reading"}, props: {posts: allPosts}},
    ]

}

const title = 'Tag';
const description = 'Categories';
const permalink = `${Astro.site.href}tag`;
---
<BaseLayout title={title} description={description} permalink={permalink} current="tag">
    <div class="container">
        <p>Hello world</p>
    </div>
  
</BaseLayout>


